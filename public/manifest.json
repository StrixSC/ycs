{
    "manifest_version": 2,
    "name": "YouTube Comment Scraper",
    "version": "1.0.0",
    "permissions": ["tabs", "activeTab"],
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "browser_action": {
        "default_popup": "popup.html"
      },
    "web_accessible_resources": ["assets/*"]
  }
